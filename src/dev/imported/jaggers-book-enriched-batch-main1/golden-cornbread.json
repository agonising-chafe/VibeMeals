    "preflight": [],
    "steps": [
      {
        "stepNumber": 1,
        "instruction": "Adjust an oven rack to the middle position and heat the oven to 375 degrees. Grease a 13 by 9-inch baking dish with butter."
      },
      {
        "stepNumber": 2,
        "instruction": "Beat the eggs in medium bowl; whisk in the buttermilk and milk."
      },
      {
        "stepNumber": 3,
        "instruction": "Whisk the cornmeal, flour, sugar, baking powder, baking soda, and salt together in a large bowl. Push the dry ingredients up the sides of the bowl to make a well, then pour the egg and milk mixture into the well and stir with a whisk until just combined; stir in the melted butter."
      },
      {
        "stepNumber": 4,
        "instruction": "Pour the batter into the prepared baking dish. Bake until the top is golden brown and the edges have pulled away from the sides of the pan, 30 to 40 minutes."
      },
      {
        "stepNumber": 5,
        "instruction": "Transfer the baking dish to a wire rack and let cool completely before using, about 1 hour."
      }
    ],
